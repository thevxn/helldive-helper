{
  "name": "helldive-helper",
  "version": "1.6.1",
  "type": "module",
  "scripts": {
    "prepare": "husky install",
    "dev": "vite",
    "build": "run-p type-check build-only",
    "build-only": "vite build",
    "preview": "vite preview",
    "type-check": "vue-tsc --noEmit -p tsconfig.cypress-ct.json --composite false",
    "lint": "eslint ./src --cache --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore && stylelint src/assets/styles/main.css --cache --fix",
    "postversion": "git push --follow-tags"
  },
  "dependencies": {
    "vue": "^3.2.41",
    "vue-router": "^4.1.5",
    "vue-select": "^4.0.0-beta.6",
    "vue-toast-notification": "^3.1.2"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.4",
    "@types/node": "*",
    "@types/nprogress": "^0.2.0",
    "@types/vue-select": "^3.16.8",
    "@vitejs/plugin-vue": "*",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/tsconfig": "^0.1.3",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.22.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.12",
    "start-server-and-test": "^1.14.0",
    "stylelint": "^15.10.2",
    "stylelint-config-standard": "^34.0.0",
    "tailwindcss": "^3.4.1",
    "typescript": "~5",
    "vite": "*",
    "vue-tsc": "^1.0.8"
  },
  "lint-staged": {
    "*.{vue,js,jsx,cjs,mjs,ts,tsx,cts,mts}": "eslint --cache ./src --fix --ignore-path .eslintignore",
    "*.{js, md}": "prettier --write",
    "main.css": "stylelint --cache --fix"
  }
}
